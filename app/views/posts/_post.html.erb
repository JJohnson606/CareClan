<div id="<%= dom_id post %>">
  <p>
    <strong>Author:</strong>
    <%= post.author_id %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= post.image %>
  </p>

  <p>
    <strong>Trusted:</strong>
    <%= post.trusted %>
  </p>

</div>
